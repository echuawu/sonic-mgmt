  <DeviceInfos>
    <DeviceInfo>
      <AutoNegotiation>true</AutoNegotiation>
      <EthernetInterfaces xmlns:a="http://schemas.datacontract.org/2004/07/Microsoft.Search.Autopilot.Evolution">
{% if card_type is not defined or card_type != 'supervisor' %}
{% set num_of_intf = port_alias | length %}
{% for index in range(num_of_intf) %}
        <a:EthernetInterface>
          <ElementType>DeviceInterface</ElementType>
          <AlternateSpeeds i:nil="true"/>
          <EnableAutoNegotiation>true</EnableAutoNegotiation>
          <EnableFlowControl>true</EnableFlowControl>
          <Index>1</Index>
          <InterfaceName>{{ port_alias[index] }}</InterfaceName>
          <InterfaceType i:nil="true"/>
          <MultiPortsInterface>false</MultiPortsInterface>
          <PortName>0</PortName>
          <Priority>0</Priority>
{% if (inventory_hostname == "arc-mtbc-1001" or inventory_hostname == "mtbc-sonic-01-2410") %}
    {% if index >= 48 %}
          <Speed>100000</Speed>
    {% else %}
          <Speed>25000</Speed>
    {% endif %}
{% else %}
    {%- if port_alias[index] in port_speed and port_speed[port_alias[index]] != ""  %}
          <Speed>{{ port_speed[port_alias[index]] }}</Speed>
    {%- else %}
        {%- if (inventory_hostname == "r-anaconda-81" or inventory_hostname == "arc-switch1025" or inventory_hostname == "mts-sonic-dut" or inventory_hostname == "mtbc-sonic-03-2700") and index >= 30 %}
          <Speed>50000</Speed>
        {%- elif inventory_hostname == "r-leopard-01" and index >= 30 %}
          <Speed>100000</Speed>
        {%- elif inventory_hostname == "arc-switch1038" %}
            {%- if index >= 16 and index <= 19 %}
          <Speed>25000</Speed>
            {%- elif index >= 20 and index <= 21 %}
          <Speed>50000</Speed>
            {%- elif index >= 30 %}
          <Speed>50000</Speed>
            {%- else %}
          <Speed>{{ iface_speed }}</Speed>
            {%- endif %}
        {%- elif inventory_hostname == "r-leopard-58" and index >= 30 %}
          <Speed>50000</Speed>
        {%- elif inventory_hostname == "r-tigris-13" %}
            {%- if index >= 0 and index <= 7 %}
          <Speed>10000</Speed>
            {%- elif index >= 62 %}
          <Speed>100000</Speed>
            {%- else %}
          <Speed>{{ iface_speed }}</Speed>
            {%- endif %}
        {%- elif inventory_hostname == "r-tigon-11" %}
            {%- if index >= 62 %}
          <Speed>50000</Speed>
            {%- else %}
          <Speed>{{ iface_speed }}</Speed>
            {%- endif %}
        {%- else %}
          <Speed>{{ iface_speed }}</Speed>
        {%- endif %}
    {%- endif %}
{%- endif %}

        </a:EthernetInterface>
{% endfor %}
{% endif %}
      </EthernetInterfaces>
{% if switch_type is defined and switch_type == 'voq' %}
      <SystemPorts>
{% set num_of_sysports = all_sysports | length %}
{% for index in range(num_of_sysports) %}
        <SystemPort>
          <Name>{{ all_sysports[index]['name'] }}</Name>
          <Hostname>{{ all_sysports[index]['hostname'] }}</Hostname>
          <AsicName>{{ all_sysports[index]['asic_name'] }}</AsicName>
          <Speed>{{ all_sysports[index]['speed'] }}</Speed>
          <SystemPortId>{{ index }}</SystemPortId>
          <SwitchId>{{ all_sysports[index]['switchid'] }}</SwitchId>
          <CoreId>{{ all_sysports[index]['coreid'] }}</CoreId>
          <CorePortId>{{ all_sysports[index]['core_portid'] }}</CorePortId>
          <NumVoq>{{ all_sysports[index]['num_voq'] }}</NumVoq>
        </SystemPort>
{% endfor %}
      </SystemPorts>
{% endif %}
      <FlowControl>true</FlowControl>
      <Height>0</Height>
      <HwSku>{{ hwsku }}</HwSku>
      <ManagementInterfaces/>
    </DeviceInfo>
  </DeviceInfos>

