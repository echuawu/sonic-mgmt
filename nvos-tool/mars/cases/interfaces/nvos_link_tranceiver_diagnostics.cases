<?xml version="1.0" encoding="UTF-8" ?>
<CASEDEF>
  <global>
    <Test>
      <group> NVOS </group>
      <name> Link Tranceiver Diagnostics </name>
      <info> </info>
      <owner> Sviatoslav Demniak </owner>
    </Test>
    <Case>
      <tags> ib port diagnostics </tags>
      <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/canonical_mars_pytest_runner.py </wrapper>
      <tout> 2400 </tout>
      <tags>ALL</tags>
    </Case>
  </global>

  <case>
    <name> Interface Tranceiver Diagnostics Basic </name>
    <info> </info>
    <tout> 2400 </tout>
    <cmd>
      <params>
          <static_args> --setup_name [[conf:extra_info.setup_name]] --test_script /root/mars/workspace/sonic-mgmt/ngts/tests_nvos/interfaces/test_interface_link_tranceiver_diagnostics.py --raw_options "\'--rootdir=/root/mars/workspace/sonic-mgmt/ngts -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini -k test_interface_tranceiver_diagnostics_basic --log-level=INFO --clean-alluredir --alluredir=/tmp/allure-results --showlocals\'"</static_args>
      </params>
    </cmd>
		<ignore>
			<condition_group>
				<condition>
					<key> split_supported </key>
					<op> not in </op>
					<value> [True] </value>
				</condition>
			</condition_group>
		</ignore>
  </case>
