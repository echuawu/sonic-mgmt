  <case>
    <name> Ib port split no breakout profile </name>
    <info> </info>
    <tout> 2400 </tout>
    <cmd>
      <params>
          <static_args> --setup_name [[conf:extra_info.setup_name]] --test_script /root/mars/workspace/sonic-mgmt/ngts/tests_nvos/interfaces/test_port_split.py --raw_options "\'--rootdir=/root/mars/workspace/sonic-mgmt/ngts -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini -k test_ib_split_port_no_breakout_profile --log-level=INFO --clean-alluredir --alluredir=/tmp/allure-results --showlocals\'"</static_args>
      </params>
    </cmd>
		<ignore>
			<condition_group>
				<condition>
					<key> split_supported </key>
					<op> not in </op>
					<value> [True] </value>
				</condition>
			</condition_group>
		</ignore>
