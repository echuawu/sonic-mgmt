<?xml version="1.0" encoding="UTF-8" ?>
<CASEDEF>
  <global>
    <Test>
      <group> NVOS </group>
      <name> LLDP Feature tests </name>
      <info> </info>
      <owner> Aleksander Romashin </owner>
    </Test>
    <Case>
      <tags> LLDP Interface </tags>
      <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/canonical_mars_pytest_runner.py </wrapper>
      <tout> 7200 </tout>
      <tags>ALL</tags>
    </Case>
  </global>

  <case>
    <name> LLDP Enabled test </name>
    <info></info>
    <tout> 600 </tout>
    <cmd>
      <params>
          <static_args> --setup_name [[conf:extra_info.setup_name]] --test_script /root/mars/workspace/sonic-mgmt/ngts/tests_nvos/interfaces/lldp/test_lldp.py  --raw_options "\'--rootdir=/root/mars/workspace/sonic-mgmt/ngts  -k test_lldp_enabled -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini --log-level=INFO --default_pass_env_var=[[conf:extra_info.default_pass_env_var]] --clean-alluredir  --alluredir=/tmp/allure-results --showlocals  [[conf:extra_info.loganalyzer_options]]  \'"</static_args>
      </params>
    </cmd>
  </case>

