<?xml version="1.0" encoding="UTF-8" ?>
<CASEDEF>
  <global>
    <Test>
      <group> NVOS </group>
      <name> NVOS - System Security Tests </name>
      <info> </info>
      <owner> Alon N. </owner>
    </Test>
    <Case>
      <tags> security </tags>
      <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/canonical_mars_pytest_runner.py </wrapper>
      <tout> 4800 </tout>
      <tags>ALL</tags>
    </Case>
  </global>

   <case>
    <name> Security cases </name>
    <info> </info>
    <cmd>
      <params>
          <static_args>  --setup_name [[conf:extra_info.setup_name]]  --test_script /root/mars/workspace/sonic-mgmt/ngts/tests_nvos/general/security/test_ssh_config/test_ssh_config.py --raw_options "\' --rootdir=/root/mars/workspace/sonic-mgmt/ngts -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini -v -m nvos_security_build --log-level=INFO --clean-alluredir --alluredir=/tmp/allure-results  --showlocals\'"</static_args>
      </params>
    </cmd>
  </case>

</CASEDEF>