<?xml version="1.0" encoding="UTF-8" ?>
<CASEDEF>
  <global>
    <Test>
      <group> Cumulus </group>
      <name> System Show Commands</name>
      <info></info>
      <owner> Yana Port </owner>
    </Test>
    <Case>
      <tags> System commands </tags>
      <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/canonical_mars_pytest_runner.py </wrapper>
      <tout> 2400 </tout>
      <tags>ALL</tags>
    </Case>
  </global>

  <case>
    <name> System Show </name>
    <info></info>
    <tout> 2400 </tout>
    <cmd>
      <params>
          <static_args> --setup_name [[conf:extra_info.setup_name]] --test_script /root/mars/workspace/sonic-mgmt/ngts/tests_nvos/system/test_show_system.py --raw_options "\' --rootdir=/root/mars/workspace/sonic-mgmt/ngts -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini -v -m cumulus --log-level=INFO --clean-alluredir --alluredir=/tmp/allure-results --skip_auto_checks --disable_cli_coverage --disable_loganalyzer --showlocals  [[conf:extra_info.loganalyzer_options]]  \'"</static_args>
      </params>
    </cmd>
  </case>

</CASEDEF>