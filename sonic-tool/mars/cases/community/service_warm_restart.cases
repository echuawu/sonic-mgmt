<?xml version="1.0" encoding="UTF-8"?>
<CASEDEF>
    <global>
        <Test>
            <info> Service Warm Restart Test </info>
            <name> service_warm_restart </name>
            <owner> </owner>
        </Test>
        <Case>
            <tags> SERVICE WARM RESTART </tags>
            <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/community_mars_pytest_runner.py  </wrapper>
            <tout> 0 </tout>
        </Case>
    </global>

    <case>
        <ignore>
            <!-- weekend case  -->
            <ignore_by_tag> TAG_ISSU_DISABLED </ignore_by_tag>
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> yes </value>
            </condition>
        </ignore>

        <info> Service Warm Restart Case </info>
        <name> service_warm_restart </name>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug  --show-capture=no -ra --showlocals --clean-alluredir --alluredir=/tmp/allure-results --timeout 3600 --session-timeout 3600 --allure_server_addr=\"10.215.11.120\"\'" --test-scripts platform_tests/test_service_warm_restart.py </static_args>
             </params>
        </cmd>

    </case>

</CASEDEF>
