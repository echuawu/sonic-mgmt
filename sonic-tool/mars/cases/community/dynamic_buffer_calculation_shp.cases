<?xml version="1.0" encoding="UTF-8"?>
<CASEDEF>
    <global>
        <Test>
            <info> Dynamic Buffer Calculation with Shared Headroom Pool Test </info>
            <name> dynamic-buffer-calculation-with-shared-headroom-pool </name>
            <owner> </owner>
        </Test>
        <Case>
            <tags> DYNAMIC_BUFFER_CALC_SHP </tags>
            <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/community_mars_pytest_runner.py  </wrapper>
            <tout> 3600 </tout>
        </Case>
    </global>

    <case>
        <ignore>
            <!-- daily case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> no </value>
            </condition>
        </ignore>
        <info> Dynamic-Buffer-Calc-SHP Case </info>
        <name> dynamic-buffer-calculation-with-shared-headroom-pool </name>
        <tout> 4800 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no --enable_shared_headroom_pool=true -ra --showlocals -k=\"not test_exceeding_headroom and not test_shared_headroom_pool_configure and not test_headroom_override and not  test_port_admin_down and not  test_lossless_pg\" --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\"\'" --test-scripts qos/test_buffer.py </static_args>
             </params>
        </cmd>
    </case>

    <case>
        <ignore>
            <!-- weekend case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> yes </value>
            </condition>
        </ignore>
        <info> Dynamic-Buffer-Calc-SHP Case </info>
        <name> dynamic-buffer-calculation-with-shared-headroom-pool weekend</name>
        <tout> 4800 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no --enable_shared_headroom_pool=true -ra --showlocals -k=\"test_headroom_override or test_port_admin_down or test_lossless_pg\" --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\"\'" --test-scripts qos/test_buffer.py </static_args>
             </params>
        </cmd>
    </case>

</CASEDEF>
