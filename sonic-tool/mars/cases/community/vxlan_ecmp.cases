<?xml version="1.0" encoding="UTF-8"?>
<CASEDEF>
    <global>
        <Test>
            <info> Overlay ECMP for T1 Test </info>
            <name> vxlan_ecmp </name>
            <owner> </owner>
        </Test>
        <Case>
            <tags> Overlay ECMP for T1 Test </tags>
            <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/community_mars_pytest_runner.py  </wrapper>
            <tout> 9000 </tout>
        </Case>
    </global>

    <case>
        <info> Overlay ECMP for T1: daily cases </info>
        <name> test_vxlan_ecmp </name>
        <tout> 7200 </tout>
        <ignore>
            <!-- daily case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> no </value>
            </condition>
        </ignore>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no -ra --showlocals --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\"\'" --test-scripts vxlan/test_vxlan_ecmp.py </static_args>
             </params>
        </cmd>
    </case>

    <case>
        <ignore>
            <!-- weekend case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> yes </value>
            </condition>
        </ignore>

        <info> Overlay ECMP for T1 weekend cases: Test_VxLAN_ecmp_random_hash </info>
        <name> Test_VxLAN_ecmp_random_hash </name>
        <tout> 7200 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no -ra --showlocals --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\" --include_long_tests=True -k \"Test_VxLAN_ecmp_random_hash\"\'" --test-scripts vxlan/test_vxlan_ecmp.py </static_args>
             </params>
        </cmd>
    </case>

    <case>
        <ignore>
            <!-- weekend case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> yes </value>
            </condition>
        </ignore>

        <info> Overlay ECMP for T1 weekend cases: Test_VxLAN_underlay_ecmp </info>
        <name> Test_VxLAN_underlay_ecmp </name>
        <tout> 7200 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no -ra --showlocals --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\" --include_long_tests=True -k \"Test_VxLAN_underlay_ecmp\"\'" --test-scripts vxlan/test_vxlan_ecmp.py </static_args>
             </params>
        </cmd>
    </case>

    <case>
        <ignore>
            <!-- weekend case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> yes </value>
            </condition>
        </ignore>

        <info> Overlay ECMP for T1 weekend cases: Test_VxLAN_entropy </info>
        <name> Test_VxLAN_entropy </name>
        <tout> 7200 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no -ra --showlocals --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\" --include_long_tests=True -k \"Test_VxLAN_entropy\"\'" --test-scripts vxlan/test_vxlan_ecmp.py </static_args>
             </params>
        </cmd>
    </case>

    <case>
        <info> Vxlan ECMP Switchover test cases </info>
        <name> test_vxlan_ecmp_switchover </name>
        <tout> 7200 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no -ra --showlocals --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\"\'" --test-scripts vxlan/test_vxlan_ecmp_switchover.py </static_args>
             </params>
        </cmd>
    </case>

</CASEDEF>