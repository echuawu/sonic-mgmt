<?xml version="1.0" encoding="UTF-8" ?>
<CASEDEF>
  <global>
    <Test>
      <group> sonic </group>
      <name> Install rpc image before run rpc test </name>
      <info> RPC Regression test</info>
      <owner> jbao </owner>
    </Test>
    <Case>
      <tags> community rpc </tags>
      <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/canonical_mars_pytest_runner.py </wrapper>
      <tout> 2500 </tout>
      <tags>ALL</tags>
    </Case>
  </global>
  <case>
    <name> install rpc image </name>
    <info> </info>
    <cmd>
      <params>
          <static_args> --setup_name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --test_script /root/mars/workspace/sonic-mgmt/ngts/scripts/sonic_deploy/test_sonic_deploy_image.py  --raw_options "\'--rootdir=/root/mars/workspace/sonic-mgmt/ngts -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini   --base_version=[[conf:extra_info.rpc_image]] --is_shutdown_bgp --deploy_type=sonic --post_validation=True --disable_loganalyzer --log-level=INFO --clean-alluredir --alluredir=/tmp/allure-results --showlocals\'"</static_args>
      </params>
    </cmd>
  </case>

</CASEDEF>
