<?xml version="1.0" encoding="UTF-8"?>
<CASEDEF>
    <global>
        <Test>
            <info> QOS Test </info>
            <name> qos_sai </name>
            <owner> </owner>
            <pre>
                <type> reg_exec_cmd </type>
                <name> work around: Disable counterpoll </name>
                <tout> 300 </tout>
                <run_posts_after_pre_failed> True </run_posts_after_pre_failed>
                <ignore_pre_fail> True </ignore_pre_fail>
                <players_selection>
                    <ep_targets> SONIC_MGMT </ep_targets>
                </players_selection>
                <params>
                    <exec> PYTHONPATH=/devts/  /ngts_venv/bin/pytest /root/mars/workspace/sonic-mgmt/ngts/scripts/test_change_counterpoll_status.py  --setup_name=[[conf:extra_info.setup_name]] --rootdir=/root/mars/workspace/sonic-mgmt/ngts -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini  --log-level=INFO --clean-alluredir --alluredir=/tmp/allure-results --disable_loganalyzer -k test_change_counterpoll_status[disable] </exec>
                </params>
            </pre>
            <post>
                <type> reg_exec_cmd </type>
                <name> work around: Enable counterpoll </name>
                <tout> 300 </tout>
                <players_selection>
                    <ep_targets> SONIC_MGMT </ep_targets>
                </players_selection>
                <params>
                    <exec> PYTHONPATH=/devts/  /ngts_venv/bin/pytest /root/mars/workspace/sonic-mgmt/ngts/scripts/test_change_counterpoll_status.py  --setup_name=[[conf:extra_info.setup_name]] --rootdir=/root/mars/workspace/sonic-mgmt/ngts -c /root/mars/workspace/sonic-mgmt/ngts/pytest.ini  --log-level=INFO --clean-alluredir --alluredir=/tmp/allure-results --disable_loganalyzer -k test_change_counterpoll_status[enable] </exec>
                </params>
            </post>
        </Test>
        <Case>
            <tags> QOS </tags>
            <wrapper> [[run_time:db_tests_path]]/sonic-mgmt/sonic-tool/mars/scripts/community_mars_pytest_runner.py  </wrapper>
            <tout> 5000 </tout>
        </Case>
    </global>

    <case>
        <ignore>
            <!-- daily case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> no </value>
            </condition>
        </ignore>
        <info> pre_tests install saithrift in ptf </info>
        <name> pre_tests install saithrift in ptf </name>
        <tout> 300 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no -ra --showlocals -v --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\"  --py_saithrift_url=\"http://fit69.mtl.labs.mlnx/auto/sw_regression/system/SONIC/MARS/conf/saithrift/python-saithrift_0.9.4_amd64.deb\" -k \"test_update_saithrift_ptf\"\'" --test-scripts test_pretest.py </static_args>
             </params>
        </cmd>
    </case>

    <case>
        <ignore>
            <!-- daily case  -->
            <condition>
                <key> skip_weekend_cases </key>
                <op> == </op>
                <value> no </value>
            </condition>
        </ignore>
        <info> QOS Case </info>
        <name> qos </name>
        <tout> 5000 </tout>
        <cmd>
             <params>
                 <static_args> --sonic-mgmt-dir /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]] --dut-name [[conf:extra_info.dut_name]] --setup-name [[conf:extra_info.setup_name]] --sonic-topo [[conf:extra_info.topology]] --json-root-dir [[conf:extra_info.json_root_dir]] --raw-options "\'--log-cli-level debug --show-capture=no -ra --showlocals --clean-alluredir --alluredir=/tmp/allure-results --allure_server_addr=\"10.215.11.120\" --port_target_speed=\"50000\" --qos_swap_syncd=False\'" --test-scripts qos/test_qos_sai.py </static_args>
             </params>
        </cmd>
    </case>

</CASEDEF>
