<?xml version="1.0" encoding="UTF-8"?>
<DBDEF>
	<global>
		<!-- parameters passed to setup steps commands-->
		<default_params_type> basic_setup_step_params </default_params_type>
		<DB>
			<info> RPC_QOS_COPP </info>
			<owner> SONiC </owner>
			<group> SONiC </group>

			<!-- OPTIONAL TAGS -->
			<tags> RPC_QOS_COPP </tags>
			<ignore>
                		<condition>
					<key> run_rpc_image </key>
					<op> == </op>
					<value>  no </value>
				</condition>
			</ignore>

			<!-- pre db commands - executed once before DB run -->
			<pre>
			</pre>


            <pre>
                <info> ptf32 topology checker </info>
                <type> reg_exec_cmd </type>
                <name> ptf32 topology checker </name>
                <tout> 30 </tout>
                <ignore_pre_fail> True </ignore_pre_fail>
                <run_posts_after_pre_failed> True </run_posts_after_pre_failed>
                <params>
                    <exec> test [[conf:extra_info.topology]] != ptf32 </exec>
                </params>

                <on_success>
                    <cmd>
                        <info> remove original  topology </info>
                        <type> reg_exec_cmd </type>
                        <name> remove original topology </name>
                        <tout> 300 </tout>
                        <params>
                            <exec> ./testbed-cli.sh -k ceos remove-topo [[conf:extra_info.dut_name]]-[[conf:extra_info.topology]] vault </exec>
                            <cmd_workdir> /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]]/ansible </cmd_workdir>
                        </params>
                    </cmd>
                    <cmd>
                        <info> add ptf-32 topology </info>
                        <type> reg_exec_cmd </type>
                        <name> add ptf-32 topology </name>
                        <tout> 1800 </tout>
                        <params>
                            <exec> ./testbed-cli.sh -k ceos add-topo [[conf:extra_info.dut_name]]-ptf32 vault </exec>
                            <cmd_workdir> /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]]/ansible </cmd_workdir>
                        </params>
                    </cmd>
                    <cmd>
                        <info> deploy minigraph of ptf-32 topology </info>
                        <type> reg_exec_cmd </type>
                        <name> deploy minigraph of ptf-32  topology </name>
                        <tout> 1200 </tout>
                        <params>
                            <exec> ./testbed-cli.sh deploy-mg [[conf:extra_info.dut_name]]-ptf32 lab vault </exec>
                            <cmd_workdir> /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]]/ansible </cmd_workdir>
                        </params>
                    </cmd>
                </on_success>
            </pre>


            <post>
                <info> original topology checker </info>
                <type> reg_exec_cmd </type>
                <name> original topology checker </name>
                <tout> 30 </tout>
                <params>
                    <exec> test [[conf:extra_info.topology]] != ptf32 </exec>
                </params>

                <on_success>
                    <cmd>
                        <info> remove ptf-32 topology </info>
                        <type> reg_exec_cmd </type>
                        <name> remove ptf-32 topology </name>
                        <tout> 300 </tout>
                        <params>
                            <exec> ./testbed-cli.sh -k ceos remove-topo [[conf:extra_info.dut_name]]-ptf32 vault </exec>
                            <cmd_workdir> /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]]/ansible </cmd_workdir>
                        </params>
                    </cmd>
                    <cmd>
                        <info> restore original topology </info>
                        <type> reg_exec_cmd </type>
                        <name> restore original topology </name>
                        <tout> 1800 </tout>
                        <params>
                            <exec> ./testbed-cli.sh -k ceos add-topo [[conf:extra_info.dut_name]]-[[conf:extra_info.topology]] vault </exec>
                            <cmd_workdir> /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]]/ansible </cmd_workdir>
                        </params>
                    </cmd>
                    <cmd>
                        <info> deploy minigraph of original topology </info>
                        <type> reg_exec_cmd </type>
                        <name> deploy minigraph of original topology </name>
                        <tout> 1200 </tout>
                        <params>
                            <exec> ./testbed-cli.sh deploy-mg [[conf:extra_info.dut_name]]-[[conf:extra_info.topology]] lab vault </exec>
                            <cmd_workdir> /root/mars/workspace/[[conf:extra_info.sonic_mgmt_repo_name]]/ansible </cmd_workdir>
                        </params>
                    </cmd>
                </on_success>
            </post>
		</DB>
		<Test>
			<pre></pre>
			<post></post>
		</Test>
		<Case>
			<!-- OPTIONAL TAGS -->
			<!--<tags> TAG NAME</tags>-->
			<pre></pre>
			<ignore>
				<condition>
					<key> run_rpc_image </key>
					<op> == </op>
					<value>  no </value>
				</condition>
			</ignore>
			<post></post>
		</Case>
	</global>

    <test>
        <cases> sonic-mgmt/sonic-tool/mars/cases/community/qos.cases </cases>
    </test>

    <test>
        <cases> sonic-mgmt/sonic-tool/mars/cases/community/copp.cases </cases>
    </test>



</DBDEF>
