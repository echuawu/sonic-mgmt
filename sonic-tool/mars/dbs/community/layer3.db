<?xml version="1.0" encoding="UTF-8"?>
<DBDEF>
    <global>
        <!-- parameters passed to setup steps commands-->
        <default_params_type> basic_setup_step_params </default_params_type>
        <DB>
            <info> Layer3 </info>
            <owner> SONiC </owner>
            <group> SONiC </group>

            <!-- OPTIONAL TAGS -->
            <tags> FIB, FIB_IPV4, FIB_IPV6, DIP_SIP, DIR_BCAST, DECAP, IP_IN_IP </tags>

            <!-- pre db commands - executed once before DB run -->
            <pre>
            </pre>
            <post></post>
        </DB>
        <Test>
            <!--Must reset any dynamic ignore tags created by other tests-->
            <pre>
                <type> reg_exec_func </type>
                <name> reset dynamic ignore tags </name>
                <mars_cmd_func> reset_dynamic_tags </mars_cmd_func>
                <tout> 60 </tout>
                <params>
                    <tags_type> ignore_tags </tags_type>
                </params>
            </pre>
            <post>
            </post>
        </Test>
        <Case>
            <!-- OPTIONAL TAGS -->
            <!--<tags> TAG NAME </tags>-->
            <pre></pre>
            <ignore></ignore>
            <post></post>
        </Case>
    </global>

    <test>
        <cases> sonic-mgmt/sonic-tool/mars/cases/community/fib.cases </cases>
    </test>

    <test>
        <cases> sonic-mgmt/sonic-tool/mars/cases/community/dip_sip.cases </cases>
    </test>

    <test>
        <cases> sonic-mgmt/sonic-tool/mars/cases/community/dir_bcast.cases </cases>
    </test>

</DBDEF>
